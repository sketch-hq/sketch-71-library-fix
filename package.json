{
  "name": "sketch-71-symbol-resync",
  "version": "1.0.0",
  "engines": {
    "sketch": ">=49.0"
  },
  "skpm": {
    "name": "Sketch 71 Symbol Resync",
    "description": "Fixes a missing Library ID issue in Sketch 71",
    "manifest": "src/manifest.json",
    "main": "sketch-71-symbol-resync.sketchplugin",
    "assets": [
      "assets/**/*"
    ],
    "sketch-assets-file": "sketch-assets/icons.sketch"
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.7.0"
  },
  "author": "Sketch <developer@sketch.com>",
  "repository": "https://github.com/sketch-hq/sketch-71-symbol-resync.git"
}
